{
    "version": 2,
    "functions": {
        "backend/app.py": {
            "runtime": "@vercel/python"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "backend/app.py"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}